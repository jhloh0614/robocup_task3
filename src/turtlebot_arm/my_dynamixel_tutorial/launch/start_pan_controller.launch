<launch>
    <!-- Start pan joint controller -->
    <rosparam file="$(find my_dynamixel_tutorial)/pan.yaml" command="load"/>
    <node name="pan_controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=dxl_manager
                --port pan_tilt_port
                pan_controller"
          output="screen"/>
</launch>
